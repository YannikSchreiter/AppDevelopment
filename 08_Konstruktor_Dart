class Essen{
  String geschmack;
  int menge;
  Essen(String geschmack, int menge){
    this.geschmack=geschmack;
      this.menge=menge;
  }
  void zeige_Eigenschaften(){
    print(this.menge);
    print(this.geschmack);
    
  }
}



main(){
Essen pizza=Essen("lecker",1);
  pizza.geschmack="ausgezeichnet";
  pizza.menge=1;
  pizza.zeige_Eigenschaften();
  Essen kotlet=Essen("saftig", 15);
  kotlet.geschmack="ausgezeichnet";
  kotlet.menge=15;
  kotlet.zeige_Eigenschaften();
}
